# logfile path
logback.logFileDir=./
# db config mysql
spring.datasource.koala.type=com.zaxxer.hikari.HikariDataSource
spring.datasource.koala.jdbc-url=jdbc:{{config.db_engine}}://{{config.db_server}}:{{config.db_port}}/{{config.db_name}}?useUnicode=true&characterEncoding=utf8&serverTimezone=Asia/Shanghai&tinyInt1isBit=false&&useSSL=false&autoReconnect=true&allowMultiQueries=true
spring.datasource.koala.driver-class-name=com.mysql.cj.jdbc.Driver
spring.datasource.koala.username={{config.db_username}}
spring.datasource.koala.password={{config.db_password}}
spring.datasource.koala.pool-name=koalaPool
spring.datasource.koala.minimum-idle=5
spring.datasource.koala.maximum-pool-size=30
spring.datasource.koala.idle-timeout=30000
spring.datasource.koala.max-lifetime=1800000
spring.datasource.koala.connection-timeout=6000
spring.datasource.koala.connection-test-query=SELECT 1


mybatis-plus.configuration.log-impl=org.apache.ibatis.logging.stdout.StdOutImpl
